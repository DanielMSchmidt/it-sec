                 return 6;
               if (getopt (3, argv, "-p") != 'p')
                 return 7;
             }
             /* This code fails on glibc 2.11.  */
             {
               char *argv[] = { "program", "-b", "-a", NULL };
               optind = opterr = 0;
               if (getopt (3, argv, "+:a:b") != 'b')
                 return 8;
               if (getopt (3, argv, "+:a:b") != ':')
                 return 9;
             }
